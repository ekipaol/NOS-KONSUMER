                        android:textAlignment="textEnd"
                        android:text="0"
                        app:suffix="%"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:maxLength="50" />

                </studio.carbonylgroup.textfieldboxes.TextFieldBoxes>

                <!-- Nominal DSR-->
